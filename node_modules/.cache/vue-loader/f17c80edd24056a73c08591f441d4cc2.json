AC;EACH,CAAC;AACH,CAAC","file":"/home/developer/Рабочий стол/quiz-app/src/components/Questions.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"questions-ctr\">\n    <div class=\"progress\">\n      <div\n        class=\"bar\"\n        :style=\"{ width: `${(answeredQuestions / questions.length) * 100}%` }\"\n      ></div>\n      <div class=\"status\">\n        {{ answeredQuestions + 1 }} из {{ questions.length }} вопросов\n      </div>\n    </div>\n    <transition-group name=\"fade\">\n      <div\n        class=\"single-question\"\n        v-for=\"(question, id) in questions\"\n        :key=\"question.q\"\n        v-show=\"answeredQuestions === id\"\n      >\n        <div class=\"question\">{{ question.q }}</div>\n        <div\n          class=\"answers\"\n          v-for=\"answer in question.answers\"\n          :key=\"answer.text\"\n          @click.prevent=\"selectAnswer(answer.is_correct)\"\n        >\n          <div class=\"answer\">{{ answer.text }}</div>\n        </div>\n      </div>\n    </transition-group>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"questions\", \"answeredQuestions\"],\n  emits: [\"selected-answer\"],\n  methods: {\n    selectAnswer(is_correct) {\n      this.$emit(\"selected-answer\", is_correct);\n    },\n  },\n};\n</script>\n\n"]}]}